let filteredQuestions = [];
let userName = "";
let selectedLang = "both";
let selectedKeyword = "";
let userAnswers = {};

const searchBox = document.getElementById("searchBox");
const userNameBox = document.getElementById("userName");
const langSelect = document.getElementById("langSelect");

const previewBox = document.getElementById("previewBox");
const quizBox = document.getElementById("quizBox");
const submitBtn = document.getElementById("submitBtn");
const resultBox = document.getElementById("resultBox");

const nextBtn = document.createElement("button");
nextBtn.textContent = "üîÑ Next key word Mock Test";
nextBtn.style.display = "none";
nextBtn.style.marginTop = "20px";
nextBtn.style.backgroundColor = "#28a745";
nextBtn.style.color = "white";
nextBtn.style.padding = "10px 20px";
nextBtn.style.border = "none";
nextBtn.style.borderRadius = "8px";
nextBtn.style.cursor = "pointer";
nextBtn.onclick = () => {
  location.reload();
};
resultBox.after(nextBtn);

const keywords = new Set();
allQuestions.forEach(q => {
  q.question_en.split(/\s+/).forEach(w => keywords.add(w.toLowerCase()));
  q.question_hi.split(/\s+/).forEach(w => keywords.add(w.toLowerCase().normalize("NFC")));
});
document.getElementById("suggestions").innerHTML = [...keywords].map(k => `<option value="${k}">`).join('');

searchBox.addEventListener("input", filterAndPreview);
userNameBox.addEventListener("input", () => {
  userName = userNameBox.value.trim();
  filterAndPreview();
});
langSelect.addEventListener("change", () => {
  selectedLang = langSelect.value;
  filterAndPreview();
});

  
  
  
function getCombinedOption(hi, en) {
  const getCore = str => str
    .replace(/^[0-9]+[.)-]?\s*/, '')    // "1. ", "2) ", "3- " ‡§Ü‡§¶‡§ø ‡§π‡§ü‡§æ‡§ì
    .replace(/[()]/g, '')              // ‡§ï‡•ã‡§∑‡•ç‡§†‡§ï ‡§π‡§ü‡§æ‡§ì
    .replace(/english/gi, '')          // "english" ‡§∂‡§¨‡•ç‡§¶ ‡§π‡§ü‡§æ‡§ì
    .trim()
    .toLowerCase();

  const hiCore = getCore(hi);
  const enCore = getCore(en);

  const matchInBracket = hi.match(/ÓÄÅ(.*?)ÓÄÅ/);
  if (matchInBracket && getCore(matchInBracket[1]) === enCore) {
    return hi.trim(); // ‚úÖ Hindi ‡§Æ‡•á‡§Ç ‡§π‡•Ä English ‡§π‡•à
  }

  if (hiCore === enCore) {
    return hi.trim(); // ‚úÖ ‡§¶‡•ã‡§®‡•ã‡§Ç ‡§è‡§ï ‡§ú‡•à‡§∏‡•á ‡§π‡•à‡§Ç
  }

  // ‚úÖ ‡§∏‡§ø‡§∞‡•ç‡§´ ‡§§‡§≠‡•Ä ‡§ú‡•ã‡§°‡§º‡•ã ‡§ú‡§¨ ‡§ä‡§™‡§∞ ‡§∏‡•á return ‡§®‡§π‡•Ä‡§Ç ‡§π‡•Å‡§Ü
  const enWithoutNumber = en.replace(/^[0-9]+[.)-]?\s*/, '').trim();
  return `${hi.trim()} / ${enWithoutNumber}`;
}

// ‚úÖ filterAndPreview ‡§Æ‡•á‡§Ç ‡§¨‡§¶‡§≤‡•á‡§Ç
function filterAndPreview() {
  const key = searchBox.value.toLowerCase().normalize("NFC");
  selectedKeyword = key;
  const msgBox = document.getElementById("message");
  msgBox.innerHTML = "";
  previewBox.innerHTML = "";
  quizBox.innerHTML = "";
  resultBox.innerHTML = "";
  submitBtn.style.display = "inline-block";
  nextBtn.style.display = "none";
  

  if (!key) return;

  filteredQuestions = allQuestions.filter(q =>
   
                                         
    q.question_en.toLowerCase().includes(key) ||
    q.question_hi.toLowerCase().normalize("NFC").includes(key)
);

  if (filteredQuestions.length === 0) {
    msgBox.innerHTML = `<div class="notice">‚ùå No questions found for this keyword.</div>`;
    return;
  }

  msgBox.innerHTML = `<div class="notice">üëâ <strong>${filteredQuestions.length} questions found for this keyword.‚¨áÔ∏è enter your name to start the mock test.</strong></div>`;

  

  const subset = filteredQuestions;
  subset.forEach((q, idx) => {
    const box = document.createElement("div");
    box.className = "question-box";
    box.innerHTML = `<b>${idx + 1}. (${q.examDate} | Q.No ${q.questionNumber})</b><br>
      ${selectedLang === 'hi' ? q.question_hi : selectedLang === 'en' ? q.question_en : q.question_hi + '<br>' + q.question_en}`;
    if (q.image) box.innerHTML += `<br><img src="${q.image}" style="max-width:100%; margin:10px 0;">`;

    const opts = selectedLang === 'hi' ? q.options_hi : selectedLang === 'en' ? q.options_en : q.options_hi.map((h, i) => getCombinedOption(h, q.options_en[i]));
    opts.forEach((opt, i) => {
      const optDiv = document.createElement("div");
      optDiv.className = "option";
      optDiv.innerHTML = `<label><input type="radio" name="q${idx}" value="${i}"> ${opt}</label>`;
      const radio = optDiv.querySelector("input");
      radio.addEventListener("change", () => { userAnswers[idx] = i; });
      box.appendChild(optDiv);
    });
    quizBox.appendChild(box);
  });
  submitBtn.style.display = "inline-block";
}


 
  
submitBtn.addEventListener("click", () => {
const userNameBox = document.getElementById("userName");
  const userName = userNameBox.value.trim();
  if (userName === "") {
    userNameBox.scrollIntoView({ behavior: "smooth", block: "center" });
    alert("‚ùó Enter Your Name");
    return;
  }
  const subset = filteredQuestions;
  for (let i = 0; i < subset.length; i++) {
    if (userAnswers[i] === undefined) {
      document.querySelectorAll(".question-box")[i].scrollIntoView({ behavior: "smooth", block: "center" });
      alert(`‚ùó Question ${i + 1} is unanswered.`);
      return;
    }
  }

  resultBox.innerHTML = "";
  let score = 0;

  quizBox.querySelectorAll(".question-box").forEach((box, idx) => {
    const q = subset[idx];
    const chosen = userAnswers[idx];
    const options = box.querySelectorAll("input[type=radio]");
   options.forEach((opt, i) => {
  const label = opt.parentElement;
  if (i === q.answer) {
    label.style.background = '#d4edda';
    label.innerHTML += ` ‚úÖ (Correct Answer)`; 
  }
  if (i === chosen && i !== q.answer) {
    label.style.background = '#f8d7da';
    label.innerHTML += ` ‚ùå(wrong answer)`;  
  }
});
    if (chosen === q.answer) score++;
  });

 resultBox.innerHTML = `<h3>üìä You answered ${score} out of ${subset.length} questions correctly.</h3>`;
  submitBtn.style.display = "none";
  submitBtn.style.display = "none";
  nextBtn.style.display = "inline-block";
});

window.onload = () => {
  document.getElementById("userName").value = "";
  document.getElementById("searchBox").value = "";
};

document.getElementById("submitBtn").addEventListener("click", function () {
  const userName = document.getElementById("userName").value.trim();
  const keyword = document.getElementById("searchBox").value.trim();
  const resultText = document.getElementById("resultBox").innerText;
  // ‡§â‡§¶‡§æ‡§π‡§∞‡§£: üìä You answered 7 out of 10 questions correctly.

  const match = resultText.match(/answered\s+(\d+\s+out\s+of\s+\d+)/i);
  const scoreText = match ? match[1] : ""; // ‡§ï‡•á‡§µ‡§≤ "7 out of 10"

  fetch("https://script.google.com/macros/s/AKfycbyirrfpB_4PrPAA1X1-QbrUDRHabrfNGu3Y9Yat9WwWEId5tAOm5eMzgwWsU-2hBn4b/exec", {
    method: "POST",
    headers: {
      "Content-Type": "application/x-www-form-urlencoded"
    },
    body: `name=${encodeURIComponent(userName)}&keyword=${encodeURIComponent(keyword)}&score=${encodeURIComponent(scoreText)}`
  })
  .then(response => response.text())
  .then(result => {
    console.log("‚úÖ ‡§°‡•á‡§ü‡§æ ‡§∏‡•á‡§µ ‡§π‡•Å‡§Ü:", result);
  })
  .catch(error => {
    console.error("‚ùå Error:", error);
  });
});
  

</script>

</body>
</html>
